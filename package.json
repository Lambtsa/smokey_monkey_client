{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --ignore-path .gitignore --write .",
    "test": "jest --watch --verbose",
    "remote:restart": "bash ./deploy/restart.sh",
    "remote:destroy": "bash ./deploy/delete.sh",
    "remote:start": "bash ./deploy/start.sh",
    "remote:deploy": "bash ./deploy/pull.sh && bash ./deploy/deploy.sh",
    "lint:check": "eslint --ignore-path .gitignore --ext .jsx,.js,.tsx,.ts ./",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:tsc": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "eslint": "8.37.0",
    "eslint-config-next": "^14.2.3",
    "mapbox-gl": "^3.3.0",
    "next": "^14.2.2",
    "nodemailer": "^6.9.13",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-intl": "^6.6.6",
    "react-use": "^17.5.0",
    "sharp": "^0.33.4",
    "styled-components": "^6.1.11",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.8",
    "babel-loader": "^9.1.2",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.0",
    "url-loader": "^4.1.1"
  }
}
